{
  "presets": [
    ["env", {"modules": false}],
    "react", "es2015","stage-1"
  ],
  "plugins": [
    "react-hot-loader/babel","transform-es2015-destructuring", "transform-object-rest-spread"
  ],
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": ["env", "react"]
    }
  }
}
